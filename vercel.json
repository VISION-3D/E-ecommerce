{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/",
      "headers": {
        "Cache-Control": "s-maxage=31536000, immutable"
      }
    }
  ],
  "env": {
    "CI": "false"
  }
}